<mvc:View
	controllerName="luigi.demo.home.Home"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
	xmlns:core="sap.ui.core"
	displayBlock="true">
	<App id="app">
		<VBox class="sapUiMediumMargin">
			<Title  class="sapUiSmallMargin" titleStyle="H1" text="Welcome to this PostMan tutorial" wrapping="true" />
			<f:Card class="sapUiSmallMargin">
				<f:content>
					<Title  class="sapUiSmallMargin" titleStyle="H4" text="Download and install Postman to get started." wrapping="true" />
				</f:content>
			</f:Card>
			<core:HTML content="&lt;a  href='https://dl.pstmn.io/download/latest/win64' &gt;&lt;img src='../common/assets/download.PNG' /&gt;&lt;/a&gt;" />
		</VBox>
	</App>
</mvc:View>
