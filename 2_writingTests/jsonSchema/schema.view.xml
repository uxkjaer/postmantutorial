<mvc:View
	controllerName="qldh.jsonSchema.schema"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"

	displayBlock="true">
	<App id="app">
		<Page class="sapUiMediumMargin"  showHeader="false">
            <content>
                <Wizard id="CreateProductWizard" class="sapUiResponsivePadding--header sapUiResponsivePadding--content"
								complete="wizardCompletedHandler">
                    <WizardStep id="step1" title="Create Request">
                        <core:Fragment
								fragmentName="qldh.jsonSchema.fragments.step1" type="XML" />
					</WizardStep>
					<WizardStep id="step2" title="Create Test">
                        <core:Fragment
								fragmentName="qldh.jsonSchema.fragments.step2" type="XML" />
					</WizardStep>
					<WizardStep id="step3" title="Evaluate results">
                        <core:Fragment
								fragmentName="qldh.jsonSchema.fragments.step3" type="XML" />
                    </WizardStep>
                </Wizard>
            </content>
        </Page>
	</App>
</mvc:View>
